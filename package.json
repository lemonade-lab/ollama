    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "openai": "^4.85.1"
  },
  "devDependencies": {
    "@alemonjs/db": "^0.0.3",
    "@alemonjs/gui": "^0.3.5",
    "@types/node": "^20.3.1",
    "alemonjs": "2.0.0-rc.88",
    "cssnano": "^7.0.6",
    "jsxp": "1.1.2",
    "globals": "^15.12.0",
    "husky": "^9.0.11",
    "less": "^4.2.1",
    "lvyjs": "^0.2.14",
    "pm2": "^5.3.0",
    "prettier": "^3.3.3",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.16"
  },
  "keywords": [
    "alemonjs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "bugs": "https://github.com/xiuxianjs/openai/issues",
