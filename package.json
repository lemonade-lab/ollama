{
  "name": "alemonjs-openai",
  "version": "0.0.2",
  "description": "openai ai",
  "author": "ningmengchongshui",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./package": "./package.json"
  },
  "scripts": {
    "dev": "lvy dev",
    "view": "lvy dev --jsxp",
